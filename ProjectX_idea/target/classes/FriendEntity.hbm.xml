<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.worker.DB_classes.FriendEntity" table="friend" schema="projectx">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="userId">
            <column name="userId" sql-type="int(6) unsigned"/>
        </property>
        <property name="friendId">
            <column name="friendId" sql-type="int(6) unsigned"/>
        </property>
        <property name="accepted">
            <column name="accepted" sql-type="tinyint(1)"/>
        </property>
    </class>
</hibernate-mapping>